				{
					"listen": "test",
					"script": {
						"id": "8a65b397-683a-4c00-9bb7-9e8d67578624",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"if(pm.response.to.have.status(200))",
							"{",
							"    var jsonData = pm.response.json();",
							"",
							"    pm.test(\"Test response.status should be 0\", function () {",
							"        pm.expect(jsonData.status).to.eql(0);",
							"    });",
							"    ",
							"    pm.test(\"Test response.message should be OK\", function () {",
							"        pm.expect(jsonData.message).to.eql('OK');",
							"    });",
							"    ",
							"    var schema = {",
							"      \"$id\": \"http://example.com/example.json\",",
							"      \"type\": \"object\",",
							"      \"definitions\": {},",
							"      \"$schema\": \"http://json-schema.org/draft-07/schema#\",",
							"      \"properties\": {",
							"        \"status\": {",
							"          \"$id\": \"/properties/status\",",
							"          \"type\": \"integer\",",
							"          \"title\": \"The Status Schema \",",
							"          \"default\": 0,",
							"          \"examples\": [",
							"            0",
							"          ]",
							"        },",
							"        \"message\": {",
							"          \"$id\": \"/properties/message\",",
							"          \"type\": \"string\",",
							"          \"title\": \"The Message Schema \",",
							"          \"default\": \"\",",
							"          \"examples\": [",
							"            \"OK\"",
							"          ]",
							"        },",
							"        \"result\": {",
							"          \"$id\": \"/properties/result\",",
							"          \"type\": \"object\",",
							"          \"properties\": {",
							"            \"address\": {",
							"              \"$id\": \"/properties/result/properties/address\",",
							"              \"type\": \"string\",",
							"              \"title\": \"The Address Schema \",",
							"              \"default\": \"\",",
							"              \"examples\": [",
							"                \"0x6b63546985a999059d6d277a9888073bda3f7b64\"",
							"              ]",
							"            },",
							"            \"valid\": {",
							"              \"$id\": \"/properties/result/properties/valid\",",
							"              \"type\": \"boolean\",",
							"              \"title\": \"The Valid Schema \",",
							"              \"default\": false,",
							"              \"examples\": [",
							"                true",
							"              ]",
							"            }",
							"          }",
							"        }",
							"      }",
							"    }",
							"",
							"    pm.test('Schema is valid', function() {",
							"        pm.expect(tv4.validate(pm.response.json(), schema)).to.be.true;",
							"    });",
							"    ",
							"}"
						],
						"type": "text/javascript"
					}
				}
